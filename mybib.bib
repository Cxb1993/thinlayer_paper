year = {2014}
}
@article{Iverson2001a,
abstract = {A new mathematical model developed to predict behavior of debris flows and avalanches also holds promise for predicting behavior of debris-laden flash floods. The model assumes that debris flows behave as mixutres of interacting Newtonian fluids and Coulomb solids. Solid and fluid constituents obey three-dimensional mass and momentum balances, which are summed and depth-integrated to yield equations that describe shallow flows of the miture as whole. An important distinction between these mixute equations and standard shallow-water equations results from strong variation of flow resistance due to interacting solid and fluid forces. Partitioning of flow reistance between solid and fluid comonents depends on fluid pressure, which evolves as flow evlosves. If fluid pressure supports the total weight of the flowing mass, all resistance results from hydrodynamic forces, and the equationsreduce to the conventional shallow-water from. If fluid pressure supports none of the weight of the flowing mass, all flow resistance results from Coulomb friction between interacting solids, and the equations describe motion of granular avalanches,. A combination of solid and fluid resistance typifies debris flows and debris-laden flash floods. In these flows solid resitance commonly is concentrated at the fronts of advancing bores that may be heavily freighted with rocks and woody debris. Rieman methods provide an effective tool for solving the shallow flow equations numerically and predicting unsteady motion of debris flows and flash floods along paths with arbitrary geometry and inclintion.},
author = {Iverson, By Richard M and Denlinger, Roger P},
file = {:home/hossein/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Iverson, Denlinger - 2001 - OF DEBRIS FLOWS AND DEBRIS-LADEN.pdf:pdf},
journal = {Proceedings of the Seventh Federal Interagency Sedimentation Conference},
pages = {1--8},
title = {{Mechanics of Debris Flows and Debris-Laden}},
url = {http://vulcan.wr.usgs.gov/Projects/MassMovement/Publications/FISC\_Mar2001/iverson\_denlinger\_fisc\_mar01.pdf},
volume = {1},
year = {2001}
}
