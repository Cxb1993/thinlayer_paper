month = dec,
number = {6 Pt B},
pages = {7186--97},
pmid = {11970661},
title = {{Phase-field model for binary alloys.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/11970661},
volume = {60},
year = {1999}
}
