@article{Stinton2004,
abstract = {The choice of an appropriate bed friction value to model simulations of natural materials is diffi cult. This problem is compounded when the application involves terrain with greatly diff erent surface properties. The Little Tahoma Peak avalanches of 1963 present an extreme case that illustrates this issue. Here the avalanches passed over the Emmons Glacier for the upper half of their extent but rode over stream gravels and glacial outwash for the lower part of their runout. The avalanches also passed over bedrock and forested soils. Intuitively the basal friction for each of these terrains should be diff erent making the selection of a representative value very diffi cult. The ability to assign specifi c bed friction angles that are appropriate for geographically distinct areas would be extremely useful. We explore this in the context of the Titan2D code which uses a depth-averaged, thin-layer, model of granular fl ow with a Coulomb-type friction term at the basal interface. The governing equations are solved on multiple processors using an adaptive mesh, Godunov scheme. Adaptive grid- ding dynamically concentrates computing power in regions of special interest ; mesh refi nement resolves key features of the avalanche. Previous simulations have shown that a single bed friction cannot adequately match velocity, pile height and runout. This inadequacy has been signifi cantly reduced by integrating a spatially variable bed friction through the use of a GIS layer. Pile velocity, runup heights and deposit morphology of simulations run with variable bed friction are closer to those of the actual avalanches.},
author = {Stinton, Aj and Sheridan, Mf and Patra, Abani K. and Dalbey, K and Namikawa, N},
journal = {Acta \ldots},
keywords = {01 to 1 km,3,are a very common,avalanches in these environments,computer model,feature of volcanic and,gis,lanches,moun-,rock avalanches,tainous environments,the range of 0,titan2d,typically have volumes in,variable bed friction},
number = {1-2},
pages = {1--11},
title = {{Incorporation of variable bed friction into Titan2D mass-flow model: Application to Little Tahoma Peak avalanches (Washington)}},
url = {http://scholar.google.com/scholar?hl=en\&btnG=Search\&q=intitle:INCORPORATION+OF+VARIABLE+BED+FRICTION+INTO+TITAN2D+MASS-FLOW+MODEL+:+APPLICATION+TO+LITTLE+TAHOMA+PEAK+AVALANCHE+(+WASHINGTON+)\#0},
