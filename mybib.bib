@article{Dalbey2008a,
abstract = {This paper presents several standard and new methods for characterizing$\backslash$nthe effect of input data uncertainty on model output for hazardous$\backslash$ngeophysical mass flows. Note that we do not attempt here to characterize$\backslash$nthe inherent randomness of such flow events. We focus here on the$\backslash$nproblem of characterizing uncertainty in model output due to lack$\backslash$nof knowledge of such input for a particular event. Methods applied$\backslash$ninclude classical Monte Carlo and Latin hypercube sampling and more$\backslash$nrecent stochastic collocation, polynomial chaos, spectral projection$\backslash$nand a newly developed extension thereof named polynomial chaos quadrature.$\backslash$nThe simple and robust samplings based Monte Carlo type methods are$\backslash$nusually computationally intractable for reasonable physical models,$\backslash$nwhile the more sophisticated and computationally efficient polynomial$\backslash$nchaos method often breaks down for complex models. The spectral projection$\backslash$nand polynomial chaos quadrature methods discussed here produce results$\backslash$nof quality comparable to the polynomial chaos type methods while$\backslash$npreserving the simplicity and robustness of the Monte Carlo-type$\backslash$nsampling based approaches at much lower cost. The computational efficiency,$\backslash$nhowever, degrades with increasing numbers of random variables. A$\backslash$nprocedure for converting the output uncertainty characterization$\backslash$ninto a map showing the probability of a hazard threshold being exceeded$\backslash$nis also presented. The uncertainty quantification procedures are$\backslash$napplied first in simple settings to illustrate the procedure and$\backslash$nthen subsequently applied to the 1991 block-and-ash flows at Colima$\backslash$nVolcano, Mexico.},
author = {Dalbey, K. and Patra, Abani K. and Pitman, E. B. and Bursik, M. I. and Sheridan, M. F.},
doi = {10.1029/2006JB004471},
file = {:home/hossein/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Dalbey et al. - 2008 - Input uncertainty propagation methods and hazard mapping of geophysical mass flows.pdf:pdf},
issn = {21699356},
journal = {Journal of Geophysical Research: Solid Earth},
keywords = {http://dx.doi.org/10.1029/2006JB004471, doi:10.102},
number = {April 2006},
pages = {1--16},
title = {{Input uncertainty propagation methods and hazard mapping of geophysical mass flows}},
