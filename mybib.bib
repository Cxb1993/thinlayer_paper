isbn = {0377-0273},
issn = {03770273},
journal = {Journal of Volcanology and Geothermal Research},
keywords = {Adaptive gridding,Avalanche,GIS,Mass-flow model,Mount Rainier,TIN},
month = jan,
number = {1-2},
pages = {89--102},
title = {{Evaluating Titan2D mass-flow model using the 1963 Little Tahoma Peak avalanches, Mount Rainier, Washington}},
