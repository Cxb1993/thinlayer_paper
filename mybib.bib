issn = {03770273},
journal = {Journal of Volcanology and Geothermal Research},
keywords = {TITAN2D,Volc\'{a}n de Colima,block-and-ash flows,volcanic hazard},
month = jun,
number = {1-2},
pages = {49--66},
title = {{Predicting the block-and-ash flow inundation areas at Volc\'{a}n de Colima (Colima, Mexico) based on the present day (February 2010) status}},
