keywords = {ENO,Gauss-Seidel,Level set method,Reinitialization,Subcell fix},
number = {8},
pages = {2764--2772},
publisher = {Elsevier Inc.},
title = {{On reinitializing level set functions}},
url = {http://dx.doi.org/10.1016/j.jcp.2009.12.032},
volume = {229},
