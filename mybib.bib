keywords = {Finite element,Finite volume,Implicit matrix-free solver,Levee overtopping,Shallow water equation,Storm surge},
month = aug,
pages = {118--128},
title = {{Hybrid numerical methods to solve shallow water equations for hurricane induced storm surge modeling}},
url = {http://www.sciencedirect.com/science/article/pii/S1364815213000546},
